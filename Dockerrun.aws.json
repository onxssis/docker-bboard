{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "birdboard",
            "image": "onxssis/birdboard-app",
            "hostname": "php",
            "essential": false
        },
        {
            "name": "nginx",
            "image": "onxssis/birdboard-nginx",
            "hostname": "nginx",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ],
            "links": ["birdboard"]
        }
    ]
}
